<!DOCTYPE html>
<html>
<head>
  <title>GoalsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>


<footer>
  <% if current_user %>
  <%= button_to "Log Out", session_url, method: :delete %>
  <% else %>
  <%= link_to "Login", new_session_url %>
  <%= link_to "Sign Up", new_user_url %>
  <% end %>
</footer>
</body>
</html>
